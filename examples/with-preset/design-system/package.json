{
  "name": "panda-mono-with-outdir-design-system",
  "version": "0.0.1",
  "private": true,
  "types": "./dist/preset.d.ts",
  "main": "./dist/preset.js",
  "exports": {
    ".": {
      "source": "./src/preset.ts",
      "types": "./dist/preset.d.ts",
      "import": {
        "types": "./dist/preset.d.mts",
        "default": "./dist/preset.mjs"
      },
      "require": "./dist/preset.js"
    }
  },
  "scripts": {
    "prepare": "pnpm panda codegen",
    "build": "tsup src/preset.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/preset.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "prettier": "^2.8.8",
    "tsup": "7.1.0",
    "typescript": "5.2.2"
  },
  "packageManager": "pnpm@8.6.4",
  "devDependencies": {
    "@pandacss/dev": "1.4.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
